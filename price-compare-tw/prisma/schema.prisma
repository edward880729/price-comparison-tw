// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model WatchProduct {
  watchProductID Int    @id @default(autoincrement())
  website        String
  keyword        String
  minPrice       Int
  maxPrice       Int
  //filter String
  sleepTime      Int
}

model SearchResult {
  searchResultID Int      @id @default(autoincrement())
  watchProductID Int
  title          String
  price          Int
  itemID         String
  url            String
  createDate     DateTime
}
